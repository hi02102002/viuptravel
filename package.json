{
  "name": "viuptravel",
  "type": "module",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.production -- next build",
    "build:dev": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local -- next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local --  next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local -- next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local -- next start",
    "migrate-dev:create": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local -- payload migrate:create",
    "migrate-dev": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.local -- payload migrate",
    "migrate-prod:create": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.production -- payload migrate:create",
    "migrate-prod": "cross-env NODE_OPTIONS=--no-deprecation dotenvx run -f .env.production -- payload migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.806.0",
    "@dotenvx/dotenvx": "^1.43.0",
    "@payloadcms/db-postgres": "3.37.0",
    "@payloadcms/email-resend": "^3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/payload-cloud": "3.37.0",
    "@payloadcms/plugin-seo": "^3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/storage-s3": "^3.37.0",
    "@payloadcms/ui": "^3.37.0",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.2",
    "@tailwindcss/postcss": "^4.1.6",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@types/lodash": "^4.17.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.510.0",
    "next": "15.3.0",
    "nuqs": "^2.4.3",
    "payload": "3.37.0",
    "postcss": "^8.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "tw-animate-css": "^1.2.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@eslint/eslintrc": "^3.2.0",
    "@next/eslint-plugin-next": "^15.3.2",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@types/node": "^22.15.17",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
